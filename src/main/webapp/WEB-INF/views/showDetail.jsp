<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1" isELIgnored="false"%>
<%@ taglib uri="http://www.springframework.org/tags" prefix="spring"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%>
<%@ page isELIgnored="false"%>
<%@ taglib prefix = "sec" uri = "http://www.springframework.org/security/tags" %>


<head>
<style>
h3
{
font-family: Lucida Handwriting;
font-size: 170%;
}
</style>
</head>


<div>
  <form action="/get">
  <input type="text"  name="keyword"  placeholder="Search" />
  <input type="submit" class="btn btn-outline-primary">search</button>
   <button type="button" class="btn btn-outline-danger">clear</button>
</form>
</div>
<div class="row">

	<div class="col-lg-12">
		<div class="card-new-heading">
		
			<h3>Details here</h3>
			<span>Salon</span> <br>
			
			<br>
			
			
		</div>
	</div>
	<h3>${info}</h3>
	<c:forEach items="${restaurents}" var="rs">
	
	 <div class="col-lg-4">
                     <div class="card fav-img">
                    <br> <img class="card-img-top"src= "data:image/jpeg;base64," alt="Card image cap">
                    <div class="card-body">
    
                      <div class="body-text text-dark">
                        <h3>${rs.name}</h3>
                         <h1>${rs.price}</h1>
                           <sec:authorize access='hasRole("ADMIN")'>
                           <a href="admin/edit?id=${rs.id}"><button class="btn btn-info">Update</button></a>
                          </sec:authorize>
                          
                         <sec:authorize access='hasRole("user")'>
                          <a href="user/api/addtocart/addfood?id=${rs.id}">  <button class="btn btn-warning">Add to Cart</button></a>
                         </sec:authorize>
                      </div>
                     
                    </div>
                   </div>
                 </div>
                 </c:forEach>

</div>